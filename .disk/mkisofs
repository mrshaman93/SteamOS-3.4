xorriso -as mkisofs -r -checksum_algorithm_iso md5,sha1 -V 'Debian 2.0 M-A 1' -o /home/buildbot/steamos/ISO.brewmaster/cd-images/debian-2.0-i386-amd64-DVD-1.iso -J -J -joliet-long -cache-inodes -isohybrid-mbr syslinux/usr/lib/ISOLINUX/isohdpfx.bin -b isolinux/isolinux.bin -c isolinux/boot.cat -boot-load-size 4 -boot-info-table -no-emul-boot -eltorito-alt-boot --efi-boot boot/grub/efi.img -append_partition 2 0x01 /home/buildbot/steamos/ISO.brewmaster/tmp/brewmaster/CD1/boot/grub/efi.img boot1 CD1
